<!-- START BLOG -->
<section id="blog" class="our_blog section-padding">
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <div class="section-title text-center">
                    <h1>Your Stories</h1>
                    <span></span>
                    <p>Lorem ipsum dolor sit amet, consectetur adipisicing elit. Voluptatem dolorem dicta libero veritatis reiciendis quis pariatur magni.</p>
                </div>
            </div>
        </div>
        <div class="row" id="yourStoriesOnHome">
            <% if (locals.lastStories && locals.lastStories.length > 0) {%>
                <% locals.lastStories.forEach(function(story){ %>
                    <div class="col-lg-4 col-sm-4 col-xs-12">
                        <div class="post-slide">
                            <div class="post-img" style="min-height: 70px; <%= story.imagePath ? '' : 'margin-left: 20px'%>">
                                <img src="<%= story.imagePath ? '/images/stories/' + story.imagePath : ''%>" alt="">
                                <div class="post-date" style="<%= story.imagePath ? '' : 'margin-top: 20px'%>">
                                    <span class="date"><%= story.dayNum %></span>
                                    <span class="month"><%= story.month %></span>
                                </div>
                            </div>
                            <div class="post-content">
                                <h3 class="post-title"><a href="/s/<%= story.id %>/<%= story.urlSlug %>"><%= story.storyName %></a></h3>
                                <p class="post-description"><%= story.storyText.substring(0, 100) %> <%= story.storyText.length > 99 ? '...': '' %></p>
                                <a href="/s/<%= story.id %>/<%= story.urlSlug %>" class="get_all_btn">read more</a>
                            </div>
                        </div>
                    </div>
                <% }) %>
            <% } else { %>
                <h5>Stories not found</h5>
            <% } %>
        </div><!--- END ROW -->
    </div><!--- END CONTAINER -->
</section>
<!-- END BLOG -->

